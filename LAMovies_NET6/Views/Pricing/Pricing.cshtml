@model IEnumerable<LAMovies_NET6.Models.Pricing>
@{
    ViewData["Title"] = "Pricing";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="iq-breadcrumb-one  iq-bg-over iq-over-dark-50" style="background-image: url(../../wwwroot/image/Background/01.jpg);">
    <div class="container-fluid">
        <div class="row align-items-center">
            <div class="col-sm-12">
                <nav aria-label="breadcrumb" class="text-center iq-breadcrumb-two">
                    <h2 class="title">Pricing</h2>
                    <ol class="breadcrumb main-bg">
                        <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Home</a></li>
                        <li class="breadcrumb-item active">Pricing</li>
                    </ol>
                </nav>
            </div>
        </div>
    </div>
</div>

<!-- Main container -->

<div class="site-content-contain">
    <div id="content" class="site-content">
        <div id="primary" class="content-area">
            <main id="main" class="site-main">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12 col-sm-12">

                            <div class="iq-pricing-card-two">
                                <div class="table-responsive iq-pricing pt-2">
                                    <table id="my-table" class="table" data-active="premium">
                                        <thead>
                                            <tr>
                                                <th class="text-center iq-price-head"></th>
                                                @foreach(var item in Model)
                                                {
                                                    <th class="text-center iq-price-head free">
                                                        <div class="iq-price-box">
                                                            <span class="">@item.namePricing</span>
                                                        </div>
                                                    </th>
                                                }
                                            </tr>
                                        </thead>
                                        <tbody>

                                            <tr>
                                                <th class="text-left" scope="row"> Time</th>
                                                @foreach (var item in Model)
                                                {
                                                    <td class="text-center iq-child-cell">
                                                        @item.timePricing month
                                                    </td>
                                                }
                                            </tr>
                                            <tr>
                                                <th class="text-left" scope="row"> Video Quality</th>
                                                <td class="text-center iq-child-cell">
                                                    Unlimited
                                                </td>
                                                <td class="text-center iq-child-cell">
                                                    Unlimited
                                                </td>
                                                <td class="text-center iq-child-cell">
                                                    Unlimited
                                                </td>
                                            </tr>
                                            <tr>
                                                <th class="text-left" scope="row"> American Tv Shows</th>
                                                <td class="text-center iq-child-cell">
                                                    <i class="fa fa-times-circle"></i>
                                                </td>
                                                <td class="text-center iq-child-cell">
                                                    <i class="fa fa-check-circle-o"></i>
                                                </td>
                                                <td class="text-center iq-child-cell">
                                                    <i class="fa fa-check-circle-o"></i>
                                                </td>
                                            </tr>
                                            <tr>
                                                <th class="text-left" scope="row"> Hollywood Movies</th>
                                                <td class="text-center iq-child-cell">
                                                    <i class="fa fa-times-circle"></i>
                                                </td>
                                                <td class="text-center iq-child-cell">
                                                    <i class="fa fa-check-circle-o"></i>
                                                </td>
                                                <td class="text-center iq-child-cell">
                                                    <i class="fa fa-check-circle-o"></i>
                                                </td>
                                            </tr>
                                            <tr>
                                                <th class="text-left" scope="row"> New Movies</th>
                                                <td class="text-center iq-child-cell">
                                                    <i class="fa fa-times-circle"></i>
                                                </td>
                                                <td class="text-center iq-child-cell">
                                                    <i class="fa fa-check-circle-o"></i>
                                                </td>
                                                <td class="text-center iq-child-cell">
                                                    <i class="fa fa-check-circle-o"></i>
                                                </td>
                                            </tr>
                                            <tr>
                                                <th class="text-left" scope="row"> Streamit Special</th>
                                                <td class="text-center iq-child-cell">
                                                    <i class="fa fa-times-circle"></i>
                                                </td>
                                                <td class="text-center iq-child-cell">
                                                    <i class="fa fa-check-circle-o"></i>
                                                </td>
                                                <td class="text-center iq-child-cell">
                                                    <i class="fa fa-times-circle"></i>
                                                </td>
                                            </tr>
                                            
                                        </tbody>
                                    </table>
                                </div>
                               
                                    <div class="iq-price-bottom">
                                        <div class="iq-select-price row">
                                        @foreach (var item in Model)
                                        {
                                            <div class="col-lg-4 col-md-6 col-sm-12">
                                                <div class="align-items-center r-mb-23" data-animation-in="fadeInUp" data-delay-in="1.3">
                                                    <a asp-controller="Payment" asp-action="PaymentPricing" asp-route-id="@item.idPricing" class="btn btn-hover iq-button">Subscribe @item.namePricing</a>
                                                </div>
                                            </div>
                                        }

                                        </div>
                                    </div>
                                
                            </div>
                        </div>
                    </div>
                </div>
            </main>
        </div>
    </div>
</div>